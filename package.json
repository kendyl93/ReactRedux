{
  "name": "reactredux",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "format": "prettier --check \"{client,server}/**/*.{js,jsx}\"",
    "format:fix": "prettier --write \"{client,server}/**/*.{js,jsx}\"",
    "libs": "scripts/libs.sh -j server/data/libraries.json -m LIBRARIES.md",
    "lint": "eslint ./ --ext \".js,.jsx\" --max-warnings=0",
    "lint:fix": "eslint ./ --ext \".js,.jsx\" --max-warnings=0 --fix",
    "stylelint": "stylelint client/**/*.scss --cache",
    "stylelint:fix": "stylelint client/src/**/*.scss --cache --fix"
  },
  "lint-staged": {
    "**/{**/!(*.min).js,*.jsx,*.json,*.scss,*.css,*.md}": [
      "prettier --write",
      "git add"
    ],
    "**/{**/!(*.min).js,*.jsx}": [
      "eslint -c .eslintrc --fix",
      "echo test",
      "git add"
    ],
    "**/*.scss": [
      "stylelint --syntax scss",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged &&  npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kendyl93/ReactRedux.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kendyl93/ReactRedux/issues"
  },
  "homepage": "https://github.com/kendyl93/ReactRedux#readme",
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.0",
    "jest": "25.1.0",
    "lint-staged": "10.0.2",
    "node-sass": "4.13.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.8.0",
    "eslint-import-resolver-webpack": "0.12.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "prettier": "1.19.1"
  },
  "dependencies": {
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  }
}
